{
    "appId": "00000000000000000000000000000013",
    "name": "Bind Tab",
    "routes": {
        "/": "tabs"
    },
    "domains": [
        "bind-tab.jillix.net",
        "bind-tab.mono.ch"
    ],
    "publicDir": "public",
    "publicRole": "0",
    "roles": [
        {
            "id": 0,
            "name": "Public"
        }
    ],
    "miids": {
        "tabs": {
            "module": "github/jillix/bind-tab/dev",
            "roles": [0],
            "config": {
                "container": "#content",
                "tabs": ".nav-tabs li",
                "options": {
                    "classes": {
                        "selected": "active"
                    },
                    "first": "tabContent1",
                    "hash": true
                },
                "html": "tabs.html",
                "css": ["//netdna.bootstrapcdn.com/twitter-bootstrap/2.3.1/css/bootstrap-combined.min.css"]
            },
            "operations": {}
        },
       "tabContent1": {
            "module": "github/jillix/bind-layout/dev",
            "roles": [0],
            "config": {
                "html": "/sample1.html"
            }
        },
        "tabContent2": {
            "module": "github/jillix/bind-layout/dev",
            "roles": [0],
            "config": {
                "html": "/sample2.html"
            }
        },
       "tabContent3": {
            "module": "github/jillix/bind-layout/dev",
            "roles": [0],
            "config": {
                "html": "/sample3.html"
            }
        }
    }
}

